{
  "name": "examples",
  "version": "0.0.1",
  "dependencies": {
    "antd": "^3.21.4",
    "handy-demo-common": "latest",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.3.1",
    "vod-fp-player": "latest",
    "vod-fp-mux": "latest"
  },
  "devDependencies": {
    "handy-service": "latest",
    "lint-staged": "^7.3.0",
    "husky": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "scripts": {
    "start": "handy-service serve",
    "build": "handy-service build"
  },
  "presets": {
    "preset": "manual",
    "features": [
      "linter",
      "antd"
    ],
    "linter": "eslint_airbnb",
    "lintCondition": "commit",
    "pkgManager": "npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/build/**.js"
    ]
  }
}
