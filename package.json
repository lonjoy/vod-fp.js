{
  "name": "vod-fp",
  "version": "0.0.1",
  "description": "MSE-EME player (hls & fmp4) , write with functional style",
  "directories": {
    "test": "tests"
  },
  "main": "lib/vod-fp.min.js",
  "scripts": {
    "dev": "DEV=player PORT=9000 webpack-dev-server --config ./scripts/webpack.dev.js --display-error-details",
    "mux": "DEV=mux PORT=9001 webpack-dev-server --config ./scripts/webpack.dev.js --display-error-details",
    "build:mux": "BUILD=mux webpack --config ./scripts/webpack.product.js --display-error-details",
    "build:player": "BUILD=player webpack --config ./scripts/webpack.product.js --display-error-details",
    "build:util": "BUILD=utility webpack --config ./scripts/webpack.product.js --display-error-details",
    "test": "mocha --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "eslint --fix packages/**/src/**.js",
        "git add"
      ]
    },
    "ignore": [
      "node_modules/**/**.js",
      "scripts/**.js",
      "packages/**/test/**.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiyuyizhi/vod-fp.js.git"
  },
  "keywords": [
    "MSE",
    "EME",
    "hls",
    "fmp4",
    "dash"
  ],
  "author": "xiyuyizhi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xiyuyizhi/vod-fp.js/issues"
  },
  "homepage": "https://github.com/xiyuyizhi/vod-fp.js#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.2",
    "eslint-config-airbnb-base": "^13.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lerna": "^3.0.0-beta.21",
    "lint-staged": "^8.1.5",
    "mocha": "^6.0.2",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "babel-eslint": "^10.0.1"
  }
}
